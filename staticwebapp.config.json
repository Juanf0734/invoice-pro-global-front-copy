{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://ebillpymetest.facturaenlinea.co/api/{*}"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.css", "/*.js", "/*.png", "/*.jpg", "/*.svg", "/*.woff2", "/api/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
